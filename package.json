{
  "name": "@alancleyton67/awesome-ui",
  "description": "Awesome Movie Database (AMDB) - React component library using Tailwind CSS",
  "author": "Alan Cleyton <alancleyton67@gmail.com>",
  "main": "dist/cjs/index.ts",
  "module": "dist/esm/index.ts",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alancleyton/awesome-ui.git"
  },
  "private": false,
  "version": "0.3.9",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build-tailwind": "tailwindcss -i ./src/index.css -o ./dist/index.css",
    "build": "npm run clean && rollup -c && npm run build-tailwind",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --docs",
    "test": "vitest",
    "test:staged": "vitest --run --passWithNoTests",
    "test:ci": "vitest --run",
    "prepare": "husky",
    "release": "changeset publish --access public"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "postcss-nesting": "^12.0.2",
    "react": ">=18",
    "react-aria-components": "^1.1.1",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "^7.6.12",
    "@storybook/react": "^7.6.12",
    "@storybook/react-vite": "^7.6.12",
    "@storybook/test": "^7.6.12",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "canvas": "^2.11.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "happy-dom": "^13.3.8",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "rollup": "^4.10.0",
    "rollup-plugin-dts": "^6.1.0",
