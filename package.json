    "vitest": "^1.2.2",
    "vitest-axe": "^0.1.0"
  },
  "bugs": {
    "url": "https://github.com/alancleyton/awesome-ui/issues"
  },
  "homepage": "https://github.com/alancleyton/awesome-ui#readme",
